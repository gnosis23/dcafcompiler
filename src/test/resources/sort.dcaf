callout printf;

int main() {
    int num[10];
    num[0] = 4;
    num[1] = 0;
    num[2] = 0;
    num[3] = 8;
    num[4] = 1;
    for (i = 0, 5) {
        for (j = i + 1, 5) {
            int k;
            if (num[j] > num[i]) {
                k = num[i];
                num[i] = num[j];
                num[j] = k;
            }
        }
    }

    for (i = 0, 5) {
        printf("%d ", num[i]);
    }
    printf("\n");
    return 0;
}